    <servlet-name>ShowDevices</servlet-name>
    <servlet-class>ua.edu.ChaliyLukyanov.laba3.model.ShowDevices</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ShowDevices</servlet-name>
    <url-pattern>/showdevices</url-pattern>
  </servlet-mapping>
